{
  "name": "doe-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod:heroku": "node -r dotenv/config dist/main.js",
    "start:prod": "pm2 start dist/main.js",
    "restart:prod": "pm2 restart main",
    "stop:prod": "pm2 stop main",
    "log": "pm2 logs",
    "monit": "pm2 monit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build-container": "docker-compose up --build --no-cache",
    "dev": "docker-compose up",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "generatemg": "yarn typeorm migration:generate -n",
    "updatedb": "yarn typeorm migration:run",
    "revertmg": "yarn typeorm migration:revert",
    "createseed": "yarn typeorm migration:create -n Seeds",
    "commit": "git-cz",
    "changelog": "echo > CHANGELOG.md && standard-version -t '' -f --changelogHeader ' '",
    "changelog:major": "echo > CHANGELOG.md && standard-version -t '' --release-as major --changelogHeader ' '",
    "changelog:minor": "echo > CHANGELOG.md && standard-version -t '' --release-as minor --changelogHeader ' '",
    "changelog:patch": "echo > CHANGELOG.md && standard-version -t '' --release-as patch --changelogHeader ' '",
    "eslint": "eslint --fix 'src/**/*.ts'"
  },
  "dependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.13",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "^3.3.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.2.4",
    "nh-utils": "^0.1.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "pm2": "^5.1.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.2.1",
    "typeorm": "^0.2.37",
    "typescript": "^4.4.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.0.6",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.39",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "git-cz": "^4.7.6",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "standard-version": "^9.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "tsconfig-paths": "^3.11.0"
  },
  "jest": {
    "bail": 1,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    }
  }
}
